# Frontend environment (local defaults)
# This file is the single source for env in this repo.

NEXT_PUBLIC_APP_NAME=CurePocket

# ==========================================
# Sui Network Configuration
# ==========================================
NEXT_PUBLIC_SUI_NETWORK=testnet

# ==========================================
# Sui Contract Configuration
# ==========================================
NEXT_PUBLIC_PACKAGE_ID=0x50163687b700f234cbe59c15cc66d97ccea01bbff087421915288026af0f319f
NEXT_PUBLIC_PASSPORT_REGISTRY_ID=0xb4fb4debd7747b364e360b20e644256bfb6c22fe1d61ec5d2b34d1de64858f24
NEXT_PUBLIC_ADMIN_CAP_ID=0xe41714d34d0e34ebd0b866642d8eb2e283af9aa70afc75591a4042e09e533e78
NEXT_PUBLIC_UPGRADE_CAP_ID=0x67df6c55bf6894a03647b02ea460d9ec3959c3f2a83bdd7c5bfc5a97450946f8

# ==========================================
# Walrus Storage Configuration
# ==========================================
# Walrus Publisher (Upload endpoint)
NEXT_PUBLIC_WALRUS_PUBLISHER_URL=https://publisher.walrus-testnet.walrus.space

# Walrus Aggregator (Download endpoint)
NEXT_PUBLIC_WALRUS_AGGREGATOR_URL=https://aggregator.walrus-testnet.walrus.space

# ==========================================
# Seal Encryption Service Configuration
# ==========================================
# KeyServer ObjectIds (comma-separated)
NEXT_PUBLIC_SEAL_KEY_SERVERS=0x164ac3d2b3b8694b8181c13f671950004765c23f270321a45fdd04d40cccf0f2
# Enable on-chain URL verification (recommended)
NEXT_PUBLIC_SEAL_VERIFY_KEY_SERVERS=true
# Key server base URL (Studio Mirai open testnet)
NEXT_PUBLIC_SEAL_SERVER_URL=https://open.key-server.testnet.seal.mirai.cloud

# ==========================================
# Application Configuration
# ==========================================
NEXT_PUBLIC_API_BASE_URL=/api
NEXT_PUBLIC_CONSENT_TOKEN_TTL_HOURS=24

# ==========================================
# Analytics Configuration
# ==========================================
NEXT_PUBLIC_ANALYTICS_ENABLED=true

# ==========================================
# Development & Debugging
# ==========================================
NEXT_PUBLIC_DEBUG=false
NEXT_PUBLIC_MOCK_STORAGE=false
